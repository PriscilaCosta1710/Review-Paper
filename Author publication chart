      "source": [
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "from google.colab import files  # Only if using Google Colab\n",
        "\n",
        "# Set academic font\n",
        "plt.rcParams['font.family'] = 'DejaVu Serif'\n",
        "\n",
        "# Author data\n",
        "authors = [\n",
        "    'Lowman, L.E.L.', 'Marengo, J.A.', 'Hoffmann, D.', 'Yuan, X.', 'Hobbins, M.',\n",
        "    'Anderson, M.C.', 'Hain, C.R.', 'Hoell, A.', 'Mishra, V.', 'Ford, T.W.',\n",
        "    'Barbosa, H.A.', 'Svoboda, M.', 'Christian, J.I.', 'Basara, J.B.', 'Hunt, E.D.', 'Otkin, J.A.'\n",
        "]\n",
        "\n",
        "publications = [4, 4, 4, 5, 5, 5, 5, 5, 6, 6, 6, 7, 7, 8, 9, 15]\n",
        "\n",
        "# Create and sort DataFrame\n",
        "df = pd.DataFrame({'Author': authors, 'Publications': publications})\n",
        "df = df.sort_values(by='Publications', ascending=True)\n",
        "\n",
        "# Plot horizontal bar chart\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "ax.barh(df['Author'], df['Publications'], color='cornflowerblue')\n",
        "\n",
        "# Labels and styling\n",
        "ax.set_xlabel('Number of publications', fontsize=12, fontweight='bold')\n",
        "ax.set_ylabel('Author name', fontsize=12, fontweight='bold')\n",
        "plt.title('Distribution of Publications by Author', fontsize=14, fontweight='bold')\n",
        "plt.xlim(0, 16)\n",
        "ax.spines['top'].set_visible(False)\n",
        "ax.spines['right'].set_visible(False)\n",
        "plt.tight_layout()\n",
        "\n",
        "# Save and download\n",
        "filename = 'author_publications.png'\n",
        "plt.savefig(filename, dpi=300, bbox_inches='tight')\n",
        "plt.show()\n",
        "files.download(filename)\n"
      ]
