        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "from google.colab import files  # Only needed in Google Colab\n",
        "\n",
        "# Set academic-style font\n",
        "plt.rcParams['font.family'] = 'DejaVu Serif'\n",
        "\n",
        "# Data (journal names and number of publications)\n",
        "journals = [\n",
        "    'Journal of Hydrometeorology',\n",
        "    'Atmosphere',\n",
        "    'Environmental Research Letters',\n",
        "    'Geophysical Research Letters',\n",
        "    'Science of The Total Environment',\n",
        "    'Bulletin of the American Meteorological Society',\n",
        "    'Environmental Research Letters ',\n",
        "    'Hydrology and Earth System Sciences',\n",
        "    'International Journal of Climatology',\n",
        "    'Remote Sensing',\n",
        "    'Water Resources Research'\n",
        "]\n",
        "\n",
        "publications = [9, 4, 4, 4, 4, 3, 3, 3, 3, 3, 3]\n",
        "\n",
        "# Create and sort DataFrame\n",
        "df = pd.DataFrame({\n",
        "    'Journal': journals,\n",
        "    'Publications': publications\n",
        "})\n",
        "df = df.sort_values(by='Publications', ascending=True)\n",
        "\n",
        "# Create horizontal bar chart\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "bars = ax.barh(df['Journal'], df['Publications'], color='cornflowerblue')\n",
        "\n",
        "# Labels and styling\n",
        "ax.set_xlabel('Number of publications', fontsize=12, fontweight='bold')\n",
        "ax.set_ylabel('Journal name', fontsize=12, fontweight='bold')\n",
        "plt.title('Distribution of Publications by Journal', fontsize=14, fontweight='bold')\n",
        "plt.xlim(0, 10)\n",
        "ax.spines['top'].set_visible(False)\n",
        "ax.spines['right'].set_visible(False)\n",
        "plt.tight_layout()\n",
        "\n",
        "# Save and automatically download\n",
        "filename = 'journal_publications.png'\n",
        "plt.savefig(filename, dpi=300, bbox_inches='tight')\n",
        "plt.show()\n",
        "\n",
        "# Automatic download in Colab\n",
        "files.download(filename)\n"
      ]
    }
